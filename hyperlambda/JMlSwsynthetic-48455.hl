// Counts messages in the "messaging_db" database from the "chat_logs" table.
data.connect:messaging_db
   database-type:sqlite

   // Counting total chat messages.
   data.read
      database-type:sqlite
      table:chat_logs
      columns
         count(*)
            as:count
      limit:long:1


