
// Create a task that executes on two specific dates, in addition to repeating every 3 hours and 5 seconds
tasks.create:foo-bar-task-3
   due:date:"2025-01-01T23:59:27.000"
   repeats:5.seconds
   repeats:3.hours
   due:date:"2030-01-01T23:59:27.000"
   .lambda
      log.info:Executing foo-bar-task-2
