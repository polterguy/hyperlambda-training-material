// How can I check user role with [if], [else-if], and [else]?
.role:string:admin
.accessLevel
if
   eq
      get-value:x:@.role
      .:admin
   .lambda
      set-value:x:@.accessLevel
         .:Full access
else-if
   eq
      get-value:x:@.role
      .:user
   .lambda
      set-value:x:@.accessLevel
         .:Limited access
else
   set-value:x:@.accessLevel
      .:No access
