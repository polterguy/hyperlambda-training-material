// How can I use [if], [else-if], and [else] for conditional branching?
.number:int:7
.output
if
   eq
      get-value:x:@.number
      .:int:5
   .lambda
      set-value:x:@.output
         .:Five
else-if
   eq
      get-value:x:@.number
      .:int:6
   .lambda
      set-value:x:@.output
         .:Six
else
   set-value:x:@.output
      .:Not Five or Six
