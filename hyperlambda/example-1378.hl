// Hyperlambda [if] and [else] with exists check
.data
   item1:value1
   item2:value2
.has-item
if
   exists:x:@.data/*/item3
   .lambda
      set-value:x:@.has-item
         .:bool:true
else
   set-value:x:@.has-item
      .:bool:false

