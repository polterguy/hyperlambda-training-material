
// KPI file for displaying the 10 last created accounts
.name:Latest accounts
.description:10 last created accounts
.enabled:bool:false
.type:table
.lambda
   data.connect:[generic|robo-crm]
      data.read
         table:accounts
         columns
            name
            status
            created
         order:created
         direction:desc
         limit:10
      return-nodes:x:-/*
