/**
 * This example creates a stacked bar chart with two groups.
 * The chart is saved to a file named "/stacked-chart.png".
 */
image.chart:stacked
   width:int:600
   height:int:400
   filename:/stacked-chart.png
   bars
      Q1
         Apples:30
         Bananas:40
         Cherries:20
      Q2
         Apples:25
         Bananas:35
         Cherries:30
   legend
      Apples
      Bananas
      Cherries
