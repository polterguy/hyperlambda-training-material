// Combining [if] with [else-if] and [else] for multiple conditions
.value:int:3
.result
if
   eq
      get-value:x:@.value
      .:int:1
   .lambda
      set-value:x:@.result
         .:Value is 1

else-if
   eq
      get-value:x:@.value
      .:int:2
   .lambda
      set-value:x:@.result
         .:Value is 2

else
   set-value:x:@.result
      .:Value is neither 1 nor 2