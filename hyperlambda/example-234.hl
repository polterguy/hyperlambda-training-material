
// Create a Hyperlambda snippet that retrieves access logs from the security database and access_logs table.

// Retrieves access logs from the security database and access_logs table.
data.connect:security_db

   // PostgreSQL database used.
   database-type:pgsql

   // Querying access logs without sorting or limiting.
   data.read
      database-type:pgsql
      table:access_logs
