
// KPI file displaying the name of the 10 last created contacts
.name:Latest contacts
.description:10 last created contacts
.enabled:bool:true
.type:table
.lambda
   data.connect:[generic|robo-crm]
      data.read
         table:contacts
         columns
            name
            created
         order:created
         direction:desc
         limit:10
      return-nodes:x:-/*
