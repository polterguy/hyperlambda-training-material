
// Create a Hyperlambda snippet that fetches users from the authentication database and its user_accounts table.

// Fetches users from the authentication database and its user_accounts table.
data.connect:auth_db

   // Database type set to PostgreSQL.
   database-type:pgsql

   // Querying the database for users.
   data.read
      database-type:pgsql
      table:user_accounts
