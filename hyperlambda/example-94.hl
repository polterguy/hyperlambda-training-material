// Limit the slots that can be invoked within a code block
.result
whitelist
   vocabulary
      set-value
      return
   .lambda
      // Allowed: this uses whitelisted [set-value]
      set-value:x:@.result
         .:foo
      // Disallowed if dot is removed: [add] is not whitelisted
      .add:x:@.result
         .
            extra:bar
      return
         status:success
