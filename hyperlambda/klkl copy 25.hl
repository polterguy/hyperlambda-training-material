// How to decide order fulfillment based on warehouse zone?
.zone:string:B
.orderStatus
if
   eq
      get-value:x:@.zone
      .:A
   .lambda
      set-value:x:@.orderStatus
         .:Priority shipping
else-if
   eq
      get-value:x:@.zone
      .:B
   .lambda
      set-value:x:@.orderStatus
         .:Standard shipping
else
   set-value:x:@.orderStatus
      .:Delayed
