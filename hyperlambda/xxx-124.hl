
// Create a Hyperlambda script to set up a call-to-action component for a 7-day free trial of E-Commerce AI Chatbots, including description, image, and other attributes.
.oninit
   set-value:x:@.image
      .:/assets/images/ai-chatbot-facts-ainiro.jpg
   set-value:x:@.description
      .:Use AI Chatbots in your E-Commerce initiatives and increase conversions and sales, while reducing your operational costs.
   set-value:x:@.image
      .:/assets/images/open-graph/ai-chatbots-og-image.png
   set-value:x:@.image-height
      .:630
.call-to-action
   io.file.mixin:/etc/www/.components/call-to-action.html
      title:7-Day Free Trial
      sub-title:E-Commerce AI Chatbots
      description:Use AI Chatbots in your E-Commerce initiatives and increase conversions and sales, while reducing your operational costs
      image:/assets/images/header/ai-chatbots.svg
      img-desc:Create an AI Chatbot based upon your data and ChatGPT, and reduce your costs while increasing sales
      width:100%
      height:auto
      class:landing-page-image
      sub-cta-link:/blog/buy-chatgpt-4-website-chatbot-for-29-dollars
      sub-cta:Get an AI chatbot for $29 per month ðŸ¥³
   return:x:-
