
// Create a Hyperlambda script to display the 10 latest created accounts as a table using data from a database.
.name:Latest accounts
.description:10 last created accounts
.enabled:bool:false
.type:table
.lambda
   data.connect:[generic|robo-crm]
      data.read
         table:accounts
         columns
            name
            status
            created
         order:created
         direction:desc
         limit:10
      return-nodes:x:-/*
